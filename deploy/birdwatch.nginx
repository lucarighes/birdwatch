server{
	listen 80;
	root /home/luca/birdwatch/build;
	index index.html;
	
	location / {
		try_files $uri $uri/ /index.html;
		add_header Cache-Control "no-cache";
	}

	location /static {
		expires 1y;
		add_header Cache-Control "public";
	}

	location /api {
		include proxy_params;
		proxy_pass http://localhost:5000;
	}
}
